# Groups - organizing code blocks
bpm 140

# Define a simple group
group intro
    print "Intro section"
    sleep 500
    print "Intro continues"
    sleep 500

# Define a verse group
group verse
    print "Verse section"
    sleep 1000

# Define a chorus group
group chorus
    print "Chorus section"
    sleep 1500

# Composition structure
print "=== Song Structure ==="
call intro
call verse
call chorus
call verse
call chorus
