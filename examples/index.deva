# This example demonstrates how to create a custom synth and use it in a group with notes.

bpm 135

bank 808

let customSynth = synth sine {
    attack: 0,
    decay: 100,
    sustain: 50,
    release: 200,
}

group mySynthGroup:
    customSynth -> note(C4, { 
        duration: 1000,
    })
    customSynth -> note(E3, { 
        duration: 1000,
    })
    customSynth -> note(D3, { 
        duration: 1000,
    })

spawn mySynthGroup